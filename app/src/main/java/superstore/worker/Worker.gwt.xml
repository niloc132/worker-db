<module>
  <inherits name="superstore.common.Common"/>
  <inherits name="com.googlecode.cqengine.CQEngine" />
  <inherits name="com.google.common.collect.Collect"/>

  <!-- Due to the use of guava, we cannot use LegacyRpcClasses, but need to rely on User -->
<!--  <inherits name="com.google.gwt.user.client.rpc.LegacyRpcClasses" />-->
  <inherits name="com.google.gwt.user.client.rpc.GwtUserFallback" />

  <inherits name="org.gwtproject.rpc.worker.RpcToWorkers" />
  <inherits name="prj.gwtwwlinker.workerlinker.WorkerLinker" />



  <source path="client" />

  <set-configuration-property name="user.agent.runtimeWarning" value="false"/>
  <set-configuration-property name="document.compatMode.severity" value="IGNORE" />

  <set-property name="user.agent" value="safari" />

  <entry-point class="superstore.worker.client.BackgroundStore" />
</module>